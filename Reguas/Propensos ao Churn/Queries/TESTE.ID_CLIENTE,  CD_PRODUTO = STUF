SELECT t2.ID_CLIENTE,  CD_PRODUTO = STUFF(
             (SELECT ',' + t1.TX_PRODUTO 
              FROM DE_TESTE_PRODUTOS t1
              WHERE t1.ID_CLIENTE = t2.ID_CLIENTE
              FOR XML PATH (''))
             , 1, 1, '') from DE_TESTE_PRODUTOS t2
group by t2.ID_CLIENTE


SELECT T1.ID_CLIENTE, CD_CAMPANHA_SECAO, CD_CAMPANHA_ACAO, CD_CAMPANHA_OFERTA, CD_CAMPANHA_MENSAGEM, CD_CAMPANHA_STATUS, NR_PRIORIDADE_SECAO, CD_CAMPANHA_CANAL, NR_PROMOCIONAL, FL_ATIVO, FL_OFERTA_COMBINADA, DT_INICIO, DT_FIM, CD_LOGOMARCA, SG_ESTADO, CD_FILIAL, CD_GRUPO, CD_CATEGORIA, CD_SUB_CATEGORIA, CD_PRODUTO_MARCA, CD_GRUPO_REMARCACAO, CD_PRODUTO, CD_PRODUTO_COMBINADO, VL_DESCONTO, VL_MINIMO, QT_MINIMA, QT_MAXIMA, PC_DESCONTO, DS_FATOR, CD_CAMPANHA_OFERTA_POOL, CD_SUBGRUPO, CD_CRM_TIPO_OFERTA, PC_DESC_INTERNET, CD_SECAO_INTERNET, QT_DIAS_VIGENCIA, DT_INICIO_INTERNET, DT_FIM_INTERNET, EXTRA
FROM DE_DH_TARGETING_PROPENSOS_CHURN_RAIA T1
INNER JOIN DE_DH_PROPENSOS_CHURN_RAIA T2
ON T1.ID_CLIENTE = T2.ID_CLIENTE
